{
    "name": "Expert Harvester",
    "description": "As an expert harvester, you can break overworld foliage with your bare hands as if you had silk touch or shears. When not toggled, it gives you guaranteed 2 sticks and a sapling from leaves, along with a 1/2 chance to drop an apple if it is oak or dark oak leaves instead while all other foliage drop their maximum drops with Fortune III. You also break them much faster. Toggled with [Primary]",
    "type": "origins:multiple",
    "toggle": {
        "type": "origins:toggle",
        "key": {
            "key": "key.origins.primary_active"
        }
    },
    "delete_bees": {
        "type": "origins:action_on_block_break",
        "only_when_harvested": false,
        "condition": {
            "type": "origins:power_active",
            "power": "*:*_toggle"
        },
        "block_condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:block",
                    "block": "minecraft:bee_nest"
                },
                {
                    "type": "origins:block",
                    "block": "minecraft:beehive"
                }
            ]
        },
        "block_action": {
            "type": "origins:execute_command",
            "command": "tp @e[type=minecraft:bee,distance=..1,sort=nearest,limit=3] ~ -100 ~"
        }
    },
    "break_faster": {
        "type": "origins:modify_break_speed",
        "block_condition": {
            "type": "origins:in_tag",
            "tag": "floran:break_fast"
        },
        "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "mainhand",
            "item_condition": {
                "type": "origins:empty"
            }
        },
        "modifier": {
            "value": 29,
            "operation": "multiply_base"
        },
        "only_when_harvested": false
    },
    "notify": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:power_active",
                "power": "*:*_toggle"
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "title @s actionbar {\"text\": \"Silk Touch Mode Toggled!\", \"color\": \"green\"}"
            },
            "else_action": {
                "type": "origins:execute_command",
                "command": "title @s actionbar {\"text\": \"Fortune Mode Toggled!\", \"color\": \"green\"}"
            }
        },
        "key": {
            "key": "key.origins.primary_active"
        }
    },
    "replace_silk": {
        "type": "origins:replace_loot_table",
        "replace": {
            "minecraft:blocks/acacia_leaves": "floran:acacia_silk",
            "minecraft:blocks/azalea_leaves": "floran:azalea_silk",
            "minecraft:blocks/birch_leaves": "floran:birch_silk",
            "minecraft:blocks/cherry_leaves": "floran:cherry_silk",
            "minecraft:blocks/dark_oak_leaves": "floran:dark_oak_silk",
            "minecraft:blocks/jungle_leaves": "floran:jungle_silk",
            "minecraft:blocks/mangrove_leaves": "floran:mangrove_silk",
            "minecraft:blocks/oak_leaves": "floran:oak_silk",
            "minecraft:blocks/spruce_leaves": "floran:spruce_silk",
            "minecraft:blocks/dead_bush": "floran:deadbush_silk",
            "minecraft:blocks/grass_block": "floran:grass_block",
            "minecraft:blocks/podzol": "floran:podzol",
            "minecraft:blocks/mycelium": "floran:mycelium",
            "minecraft:blocks/fern": "floran:fern",
            "minecraft:blocks/large_fern": "floran:fern",
            "minecraft:blocks/grass": "floran:grass_silk",
            "minecraft:blocks/glow_lichen": "floran:glow_lichen",
            "minecraft:blocks/hanging_roots": "floran:hanging_roots",
            "minecraft:blocks/seagrass": "floran:seagrass",
            "minecraft:blocks/tall_seagrass": "floran:seagrass",
            "minecraft:blocks/brown_mushroom_block": "floran:brown_mushroom_silk",
            "minecraft:blocks/red_mushroom_block": "floran:red_mushroom_silk",
            "minecraft:blocks/melon": "floran:melon_silk",
            "minecraft:blocks/bee_nest": "floran:bee_nest",
            "minecraft:blocks/beehive": "floran:beehive",
            "minecraft:blocks/vine": "floran:vine"
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "mainhand",
                    "item_condition": {
                        "type": "origins:empty"
                    }
                },
                {
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                }
            ]
        }
    },
    "replace_fortune": {
        "type": "origins:replace_loot_table",
        "replace": {
            "minecraft:blocks/acacia_leaves": "floran:acacia",
            "minecraft:blocks/azalea_leaves": "floran:azalea",
            "minecraft:blocks/birch_leaves": "floran:birch",
            "minecraft:blocks/cherry_leaves": "floran:cherry",
            "minecraft:blocks/dark_oak_leaves": "floran:dark_oak",
            "minecraft:blocks/jungle_leaves": "floran:jungle",
            "minecraft:blocks/mangrove_leaves": "floran:mangrove",
            "minecraft:blocks/oak_leaves": "floran:oak",
            "minecraft:blocks/spruce_leaves": "floran:spruce",
            "minecraft:blocks/dead_bush": "floran:deadbush",
            "minecraft:blocks/fern": "floran:grass",
            "minecraft:blocks/large_fern": "floran:grass",
            "minecraft:blocks/grass": "floran:grass",
            "minecraft:blocks/melon": "floran:melon",
            "minecraft:blocks/sweet_berry_bush": "floran:sweet_berries",
            "minecraft:blocks/wheat": "floran:wheat",
            "minecraft:blocks/beetroots": "floran:beetroot",
            "minecraft:blocks/carrots": "floran:carrot",
            "minecraft:blocks/potatoes": "floran:potato",
            "minecraft:blocks/nether_wart": "floran:nether_wart",
            "minecraft:blocks/brown_mushroom_block": "floran:brown_mushroom",
            "minecraft:blocks/red_mushroom_block": "floran:red_mushroom"
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "mainhand",
                    "item_condition": {
                        "type": "origins:empty"
                    }
                },
                {
                    "inverted": true,
                    "type": "origins:power_active",
                    "power": "*:*_toggle"
                }
            ]
        }
    }
}